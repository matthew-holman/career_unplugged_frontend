{
  "name": "career_unplugged-frontend",
  "version": "0.0.1",
  "description": "frontend for viewing scraped job results",
  "productName": "career_unplugged-frontend",
  "author": "Matthew Holman <mholman000@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "lint-fix": "eslint --fix --max-warnings 0 --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "typecheck": "vue-tsc -p tsconfig.vue-tsc.json --noEmit",
    "dev": "quasar dev",
    "build": "quasar build",
    "generate-client": "openapi --input http://0.0.0.0:5005/openapi.json --output ./src/client/scraper --client axios",
    "prepare": "husky install"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "axios": "^1.6.0",
    "pinia": "^2.1.4",
    "quasar": "^2.8.0",
    "vue": "^3.4.18",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.8.0",
    "@types/node": "^20.19.29",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "openapi-typescript-codegen": "^0.25.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "vite-plugin-checker": "^0.6.4",
    "vue-tsc": "^1.8.22"
  },
  "engines": {
    "node": "^20 || ^18 || ^16",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --max-warnings 0"
  }
}
